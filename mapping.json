{
  "settings": {
    "number_of_shards": 30,
    "number_of_replicas": 0,
    "refresh_interval": "60s",
    "translog.durability": "async",
    "translog.sync_interval": "30s",
    "translog.flush_threshold_size": "1024m"
  },
  "mappings": {
    "doc": {
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "dateOptionalTime"
        },
        "method": {
          "type": "text"
        },
        "host": {
          "type": "text"
        },
        "path": {
          "type": "text"
        },
        "ip": {
          "type": "text"
        },
        "response_time": {
          "type": "text"
        },
        "at": {
          "type": "text"
        },
        "at2": {
          "type": "text"
        },
        "at3": {
          "type": "text"
        },
        "traceno": {
          "type": "text"
        },
        "message": {
          "type": "text"
        }
      }
    }
  }
}